{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"styles":{
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--paragraph)",
			"fontSize": "var(--wp--preset--font-size--medium)"
		}
	},
	"settings": {
		"layout": {
			"contentSize": "1320px"
		},
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"name": "Black",
					"slug": "black",
					"color": "#222"
				},
				{
					"name": "White",
					"slug": "white",
					"color": "#FFF"
				},
				{
					"name": "Red",
					"slug": "primary",
					"color": "#F00"
				},
				{
					"name": "Green",
					"slug": "secondary",
					"color": "#0F0"
				},
				{
					"name": "Blue",
					"slug": "tertiary",
					"color": "#00F"
				}
			]
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"customSpacingSize": false,
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Small",
					"slug": "small",
					"size": "clamp(.5rem, .0625rem + .875vw, .9375rem)"
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "clamp(.9375rem, 0rem + 1.875vw, 1.875rem)"
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "clamp(1.875rem, .625rem + 2.5vw, 3.125rem)"
				}
			]
		},
		"typography": {
			"customFontSize": false,
			"defaultFontSizes": false,
			"dropCap": false, 
			"fontStyle": false,
			"fontWeight": false,
			"textDecoration": false,
			"letterSpacing": false,
			"fontSizes": [
				{
					"name": "Small",
					"slug": "small",
					"size": "clamp(1rem, .875rem + .25vw, 1.125rem)"
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "clamp(1.25rem, 1rem + .5vw, 1.5rem)"
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "clamp(1.5rem, 1rem + 1vw, 2rem)"
				}
			],
			"fontFamilies": [
				{
					"fontFamily": "\"Nunito\", arial, sans-serif",
					"name": "Nunito",
					"slug": "paragraph",
					"fontFace": [
						{
							"fontFamily": "Nunito",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/nunito/nunito-regular.woff2",
								"file:./assets/fonts/nunito/nunito-regular.woff",
								"file:./assets/fonts/nunito/nunito-regular.eot",
								"file:./assets/fonts/nunito/nunito-regular.svg",
								"file:./assets/fonts/nunito/nunito-regular.ttg"
							]
						},
						{
							"fontFamily": "Nunito",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/nunito/nunito-italic.woff2",
								"file:./assets/fonts/nunito/nunito-italic.woff",
								"file:./assets/fonts/nunito/nunito-italic.eot",
								"file:./assets/fonts/nunito/nunito-italic.svg",
								"file:./assets/fonts/nunito/nunito-italic.ttg"
							]
						},
						{
							"fontFamily": "Nunito",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/nunito/nunito-bold.woff2",
								"file:./assets/fonts/nunito/nunito-bold.woff",
								"file:./assets/fonts/nunito/nunito-bold.eot",
								"file:./assets/fonts/nunito/nunito-bold.svg",
								"file:./assets/fonts/nunito/nunito-bold.ttg"
							]
						},
						{
							"fontFamily": "Nunito",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/nunito/nunito-bolditalic.woff2",
								"file:./assets/fonts/nunito/nunito-bolditalic.woff",
								"file:./assets/fonts/nunito/nunito-bolditalic.eot",
								"file:./assets/fonts/nunito/nunito-bolditalic.svg",
								"file:./assets/fonts/nunito/nunito-bolditalic.ttg"
							]
						}
					]
				}
			]
		}
	}
}
